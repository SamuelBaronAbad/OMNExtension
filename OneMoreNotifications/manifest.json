{
    "name": "OneMoreNotification",
    "version": "1.0",
    "description": "Use this to see all your social networks in your browser",
    "manifest_version": 2,
    "author": "Samuel Bar√≥n Abad",
    "icons": {
        "16": "assets/img/png/icon16.png",
        "48": "assets/img/png/icon48.png",
        "128": "assets/img/png/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "assets/img/png/icon19.png"
        },
        "default_title": "OneMoreNotification",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "permissions": [
        "notifications",
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*",
        "cookies",
        "https://www.youtube.com/*",
        "https://www.facebook.com/*"
    ]
}